function addpermission_mapping(permission_mappingPermissions_id, permission_mappingResource_id) {
	log.info("add permission_mapping");
	if(permission_mappingPermissions_id != null && permission_mappingResource_id != null ){
	var db_qury = db_query_permission_mapping_add(null ,permission_mappingPermissions_id, permission_mappingResource_id);
