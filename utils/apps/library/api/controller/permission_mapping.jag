		return invokeError("Please enter permission_mappingPermissions_id, permission_mappingResource_id");
	}
}

function updatepermission_mapping(permission_mappingId,permission_mappingPermissions_id, permission_mappingResource_id) {
	log.info("update permission_mapping");
	if(permission_mappingId != null  && permission_mappingPermissions_id != null && permission_mappingResource_id != null ){
	var db_qury = db_query_permission_mapping_update(permission_mappingId ,permission_mappingPermissions_id, permission_mappingResource_id);
	log.info(db_qury);
